    "next-sanity": "^9.8.60",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss-animated": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "onchange": "^7.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "tailwindcss": "^4.0.9",
    "typescript": "^5"
